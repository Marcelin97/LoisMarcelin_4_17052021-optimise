<%- include ('../pages/header') %>

<div class="ui comments">
  <div class="comment">
    <a class="avatar" href="/message/<%= message.id %>">
      <i class="user icon"></i>
    </a>
    <div class="content">
      <div class="metadata">
        <div class="date">
          <%= message.created_at.fromNow() %>
        </div>
      </div>
      <div class="test">
        <%= message.content %>
      </div>
    </div>
  </div>
</div>

<%- include('../pages/footer') %>